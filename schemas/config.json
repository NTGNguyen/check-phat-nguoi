{
  "$defs": {
    "ApiEnum": {
      "enum": [
        "all",
        "checkphatnguoi.vn",
        "csgt.vn"
      ],
      "title": "ApiEnum",
      "type": "string"
    },
    "PlateInfoDTO": {
      "properties": {
        "plate": {
          "description": "Biển số",
          "examples": [
            "60A64685",
            "98-A-56604",
            "30-F88251",
            "59XB-00000"
          ],
          "title": "Biển số",
          "type": "string"
        },
        "owner": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Ghi chú chủ sở hữu (phù hợp khi dùng notify ai đó)",
          "examples": [
            "@kevinnitro",
            "dad"
          ],
          "title": "Ghi chú chủ sở hữu"
        },
        "type": {
          "$ref": "#/$defs/VehicleTypeEnum",
          "description": "Loại phương tiện để gửi request cũng như lọc loại phương tiện đối với các API không lọc loại phương tiện sẵn",
          "title": "Loại phương tiện"
        },
        "api": {
          "anyOf": [
            {
              "$ref": "#/$defs/ApiEnum"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Sử dụng API từ trang web nào (để trống sẽ sử dụng API define ở scope ngoài)",
          "title": "API"
        }
      },
      "required": [
        "plate",
        "type"
      ],
      "title": "PlateInfoDTO",
      "type": "object"
    },
    "TelegramDTO": {
      "properties": {
        "botToken": {
          "description": "Bot token Telegram",
          "examples": [
            "2780473231:weiruAShGUUx4oLOMoUhd0GiREXSZcCq-uB"
          ],
          "title": "Bottoken",
          "type": "string"
        },
        "chatId": {
          "description": "Chat ID Telegram",
          "examples": [
            "-1001790012349"
          ],
          "title": "Chatid",
          "type": "string"
        }
      },
      "required": [
        "botToken",
        "chatId"
      ],
      "title": "TelegramDTO",
      "type": "object"
    },
    "TelegramNotifyDTO": {
      "properties": {
        "enabled": {
          "default": true,
          "description": "Kích hoạt",
          "title": "Enabled",
          "type": "boolean"
        },
        "telegram": {
          "$ref": "#/$defs/TelegramDTO",
          "description": "Gửi thông báo đến telegram"
        }
      },
      "required": [
        "telegram"
      ],
      "title": "TelegramNotifyDTO",
      "type": "object"
    },
    "VehicleTypeEnum": {
      "enum": [
        1,
        2,
        3
      ],
      "title": "VehicleTypeEnum",
      "type": "integer"
    }
  },
  "properties": {
    "plates": {
      "description": "Danh sách các biển xe",
      "items": {
        "$ref": "#/$defs/PlateInfoDTO"
      },
      "title": "Plates",
      "type": "array"
    },
    "notifications": {
      "description": "Danh sách các thiết lập để thông báo",
      "items": {
        "$ref": "#/$defs/TelegramNotifyDTO"
      },
      "title": "Notifications",
      "type": "array"
    },
    "api": {
      "$ref": "#/$defs/ApiEnum",
      "default": "checkphatnguoi.vn",
      "description": "Sử dụng API từ trang web nào (mặc định sử dụng API từ trang checkphatnguoi.vn)",
      "title": "API"
    },
    "unpaidOnly": {
      "default": true,
      "description": "Chỉ hiển thị thông tin vi phạm chưa nộp phạt",
      "title": "Unpaidonly",
      "type": "boolean"
    },
    "verbose": {
      "default": false,
      "description": "Hiển thị tất cả thông tin có thể hiển thị",
      "title": "Verbose",
      "type": "boolean"
    },
    "requestTimeout": {
      "default": 10,
      "description": "Thời gian (s) để gửi request đến server API và gửi message notify",
      "title": "Requesttimeout",
      "type": "integer"
    },
    "detailLog": {
      "default": true,
      "title": "Detaillog",
      "type": "boolean"
    },
    "logLevel": {
      "default": "WARNING",
      "enum": [
        "NOTSET",
        "DEBUG",
        "INFO",
        "WARNING",
        "ERROR",
        "CRITICAL"
      ],
      "title": "Loglevel",
      "type": "string"
    }
  },
  "required": [
    "plates",
    "notifications"
  ],
  "title": "ConfigDTO",
  "type": "object"
}