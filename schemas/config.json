{
  "$defs": {
    "ApiEnum": {
      "enum": [
        "checkphatnguoi.vn",
        "csgt.vn"
      ],
      "title": "ApiEnum",
      "type": "string"
    },
    "LogLevelEnum": {
      "enum": [
        "NOTSET",
        "DEBUG",
        "INFO",
        "WARNING",
        "ERROR",
        "CRITICAL"
      ],
      "title": "LogLevelEnum",
      "type": "string"
    },
    "PlateInfoDTO": {
      "properties": {
        "plate": {
          "description": "Biển số",
          "examples": [
            "60A64685",
            "98-A-56604",
            "30-F88251",
            "59XB-00000"
          ],
          "title": "Biển số",
          "type": "string"
        },
        "owner": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Ghi chú chủ sở hữu (phù hợp khi dùng notify ai đó)",
          "examples": [
            "@kevinnitro",
            "dad"
          ],
          "title": "Ghi chú chủ sở hữu"
        },
        "type": {
          "anyOf": [
            {
              "$ref": "#/$defs/VehicleTypeEnum"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Loại phương tiện. Khi sử dụng API \"checkphatnguoi_vn\" không cần trường này",
          "title": "Loại phương tiện"
        },
        "api": {
          "anyOf": [
            {
              "$ref": "#/$defs/ApiEnum"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Sử dụng API từ trang web nào (để trống sẽ sử dụng API define ở scope ngoài)",
          "title": "API"
        }
      },
      "required": [
        "plate"
      ],
      "title": "PlateInfoDTO",
      "type": "object"
    },
    "TelegramDTO": {
      "properties": {
        "bot_token": {
          "description": "Bot token Telegram",
          "examples": [
            "2780473231:weiruAShGUUx4oLOMoUhd0GiREXSZcCq-uB"
          ],
          "title": "Bot Token",
          "type": "string"
        },
        "chat_id": {
          "description": "Chat ID Telegram",
          "examples": [
            "-1001790012349"
          ],
          "title": "Chat Id",
          "type": "string"
        }
      },
      "required": [
        "bot_token",
        "chat_id"
      ],
      "title": "TelegramDTO",
      "type": "object"
    },
    "TelegramNotifyDTO": {
      "properties": {
        "enabled": {
          "default": true,
          "description": "Kích hoạt",
          "title": "Enabled",
          "type": "boolean"
        },
        "telegram": {
          "$ref": "#/$defs/TelegramDTO"
        }
      },
      "required": [
        "telegram"
      ],
      "title": "TelegramNotifyDTO",
      "type": "object"
    },
    "VehicleTypeEnum": {
      "enum": [
        1,
        2,
        3
      ],
      "title": "VehicleTypeEnum",
      "type": "integer"
    }
  },
  "properties": {
    "plates": {
      "description": "Danh sách các biển xe",
      "items": {
        "$ref": "#/$defs/PlateInfoDTO"
      },
      "title": "Plates",
      "type": "array"
    },
    "notifications": {
      "description": "Danh sách các thiết lập để thông báo",
      "items": {
        "$ref": "#/$defs/TelegramNotifyDTO"
      },
      "title": "Notifications",
      "type": "array"
    },
    "api": {
      "$ref": "#/$defs/ApiEnum",
      "default": "checkphatnguoi.vn",
      "description": "Sử dụng API từ trang web nào (mặc định sử dụng API từ trang checkphatnguoi.vn)",
      "title": "API"
    },
    "unpaid_only": {
      "default": true,
      "description": "Chỉ hiển thị thông tin vi phạm chưa nộp phạt",
      "title": "Unpaid Only",
      "type": "boolean"
    },
    "verbose": {
      "default": false,
      "description": "Hiển thị tất cả thông tin có thể hiển thị",
      "title": "Verbose",
      "type": "boolean"
    },
    "detail_log": {
      "default": true,
      "title": "Detail Log",
      "type": "boolean"
    },
    "log_level": {
      "$ref": "#/$defs/LogLevelEnum",
      "default": "INFO"
    }
  },
  "title": "ConfigDTO",
  "type": "object"
}